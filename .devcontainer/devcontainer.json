{
  "name": "timesformer-dev",
  "dockerComposeFile": ["../../Jabez/docker-compose.yml"],
  "service": "timesformer",
  "workspaceFolder": "/workspace/TimeSformer",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python", "ms-toolsai.jupyter"],
      "settings": {
        "python.defaultInterpreterPath": "/opt/conda/envs/timesformer/bin/python"
      }
    }
  },
  "runServices": ["timesformer"],
  "shutdownAction": "stopCompose"
}
